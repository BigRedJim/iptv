<addons>
<addon id="iptv" name="IPTV" version="0.0.4" provider-name="BigRedJim">
    <extension point="xbmc.addon.repository" name="Example Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/BigRedJim/iptv/master/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/BigRedJim/iptv/master/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/BigRedJim/iptv/master/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>iptv</summary>
        <description>All your IPTV addons</description>
        <disclaimer></disclaimer>
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon>

